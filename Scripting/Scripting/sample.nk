Func printSpecies(animal)
if (animal = Animal.Cat)
	print "\n"
	print "Macsek"
if (animal = Animal.Dog)
	print "\n"
	print "Doggo"
if (animal = Animal.Monkey)
	print "\n"
	print "Makesz"
delete animal

START
create Samu Animal.Cat
create Ropi Animal.Dog
Call printSpecies(Samu)
Call printSpecies(Ropi)
print "\n"
delete Samu
delete Ropi
END